void makeon(int c0,int c1,int c2,int xt,int yt,int xb,int yb,int fo,int st,int si,int c3,int xs,int ys,char strfo[10] )
	{
	setlinestyle(0,1,1);
	settextstyle(fo,st,si);
	setcolor(c0);
	rectangle(xt,yt,xb,yb);
	setfillstyle(1,c2);
	floodfill(xt+7,yt+7,c0);
	setcolor(c1);
	rectangle(xt+5,yt+4,xb-4,yb-5);
	setfillstyle(1,c1);
	floodfill(xt+7,yt+6,c1);
	line(xb,yt,xb-4,yt+4);
	line(xt,yb,xt+4,yb-5);
	rectangle(xt,yt,xb,yb);
	setfillstyle(1,c0);
	floodfill(xt+3,yt+7,c1);
	setfillstyle(1,c2);
	floodfill(xb-2,yb-5,c1);
	setcolor(c3);outtextxy(xs,ys,strfo);
	}

int  getdigit(int x,int y,int down,int up)
	{
	int t,gg=0;
	int n,set,xl,yl;
	n=up-down;
	xl=x+6;yl=y+15;
	if(n>=100) set=3;
	  else set=2;
	for(t=0;t<4;t++)
	    dig[t]=NULL;
	settextstyle(0,0,1);
	setlinestyle(0,1,1);
	setcolor(15);
	line(xl,yl,xl+8,yl);
	while( ( ch=getch() )!=13 )
		{
		if(ch==27) return(-1);
		 else if( (ch=='\b') && gg==0 )
		 {setcolor(15);setlinestyle(0,1,1);
		 line(xl,yl,xl+8,yl);xl=x+6;yl=y+15;
		 sound(5000);delay(10);nosound();
		 }
		 else if( ch=='\b' && gg>0 )
		 {
		 setcolor(0);
		 line(xl,yl,xl+8,yl);
		 settextstyle(0,0,1);
		 outtextxy(x+6,y+8,dig);
		 setcolor(15);
		 xl=xl-8;
		 line(xl,yl,xl+8,yl);
		 dig[--gg]=NULL;
		 }
		 else if(ch!='\b' && gg>=set)
		 {
		  setcolor(15);setlinestyle(0,1,1);
		 line(xl,yl,xl+8,yl);
		 sound(5000);delay(10);nosound();
		 }
		 else if(ch>47 && ch<58 )
		 {
		 dig[gg]=ch;dig[++gg]=NULL;
		 setcolor(0);
		 line(xl,yl,xl+8,yl);
		 setcolor(15);
		 xl=xl+8;
		 line(xl,yl,xl+8,yl);
		 }

		 setcolor(15);
		 settextstyle(0,0,1);
		 outtextxy(x+6,y+8,dig);
		 line(xl,yl,xl+8,yl);
	   }/*end while*/
	setcolor(0);
	line(xl,yl,xl+8,yl);
	return(1);
       }

